CREATE TABLE ONDEPARTAMENTO (ID NUMBER(10) NOT NULL, ACTIVO NUMBER(1), NOMBRE VARCHAR, VERSION NUMBER(10), PRIMARY KEY (ID))
CREATE TABLE ONEMPLEADO (ID NUMBER(10) NOT NULL, EMPLEADO_TYPE VARCHAR(31), ACTIVO NUMBER(1), APELLIDOS VARCHAR, DNI VARCHAR, NOMBRE VARCHAR, VERSION NUMBER(10), DEPARTAMENTO_ID NUMBER(10), HORAS NUMBER(10), SALARIOPORHORA NUMBER(10,5), IMPUESTO NUMBER(10,5), SALARIO NUMBER(10,5), PRIMARY KEY (ID))
ALTER TABLE ONEMPLEADO ADD CONSTRAINT FK_ONEMPLEADO_DEPARTAMENTO_ID FOREIGN KEY (DEPARTAMENTO_ID) REFERENCES ONDEPARTAMENTO (ID)
